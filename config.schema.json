{
  "pluginAlias": "PowerShades",
  "pluginType": "platform",
  "singular": true,
    "headerDisplay": "<p align='center'><img width='400px' src='data:image/png;base64,'></p><br><p>A Homebridge plugin for PowerShades motorized window shades with adaptive polling, HTTP keep-alive, and intelligent caching for optimal performance.</p>",
  "footerDisplay": "Visit [PowerShades Dashboard](https://dashboard.powershades.com) to manage your shades and view device status.",
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Platform Name",
        "type": "string",
        "default": "PowerShades",
        "description": "The name that will appear in your Homebridge log."
      },
      "apiToken": {
        "title": "API Token (Recommended)",
        "type": "string",
        "description": "Your PowerShades API token. Get one from: Dashboard → Click your name (top right) → My Account → Authorized Applications → 'Get New API Token'. Using an API token is more secure than storing your password.",
        "x-schema-form": {
          "type": "password"
        }
      },
      "email": {
        "title": "Email (if not using API token)",
        "type": "string",
        "format": "email",
        "description": "Your PowerShades account email address. Only required if not using an API token."
      },
      "password": {
        "title": "Password (if not using API token)",
        "type": "string",
        "description": "Your PowerShades account password. Only required if not using an API token.",
        "x-schema-form": {
          "type": "password"
        }
      },
      "baseUrl": {
        "title": "API Base URL",
        "type": "string",
        "placeholder": "https://api.powershades.com",
        "description": "Custom API endpoint (leave blank for default)."
      },
      "pollInterval": {
        "title": "Idle Polling Interval",
        "type": "integer",
        "default": 10,
        "minimum": 2,
        "maximum": 60,
        "description": "How often (in seconds) to check shade status when idle. Default is 10 seconds."
      },
      "fastPollInterval": {
        "title": "Active Polling Interval",
        "type": "integer",
        "default": 1,
        "minimum": 1,
        "maximum": 5,
        "description": "How often (in seconds) to check shade status after controlling a shade. Default is 1 second for instant feedback."
      },
      "fastPollDuration": {
        "title": "Active Polling Duration",
        "type": "integer",
        "default": 30,
        "minimum": 5,
        "maximum": 120,
        "description": "How long (in seconds) to use fast polling after activity before returning to idle interval. Default is 30 seconds."
      },
      "shadeListCacheTTL": {
        "title": "Shade List Cache Duration",
        "type": "integer",
        "default": 300,
        "minimum": 60,
        "maximum": 3600,
        "description": "How long (in seconds) to cache the shade list. Default is 300 seconds (5 minutes)."
      }
    },
    "required": ["name"]
  },
  "layout": [
    {
      "type": "section",
      "title": "Account Settings",
      "expandable": false,
      "items": [
        "name",
        {
          "type": "help",
          "helpvalue": "<p><strong>Authentication:</strong> Provide either an <strong>API Token</strong> (recommended) OR your <strong>Email and Password</strong>.</p>"
        },
        "apiToken",
        "email",
        "password"
      ]
    },
    {
      "type": "section",
      "title": "Advanced Settings",
      "expandable": true,
      "expanded": false,
      "items": [
        {
          "key": "baseUrl",
          "description": "Only change this if you're using a custom PowerShades API endpoint."
        },
        {
          "type": "help",
          "helpvalue": "<h5>Polling Configuration</h5><p>The plugin uses adaptive polling for optimal performance:</p><ul><li><strong>Active Polling:</strong> Fast updates right after you control a shade</li><li><strong>Idle Polling:</strong> Slower updates when no recent activity</li></ul>"
        },
        "pollInterval",
        "fastPollInterval",
        "fastPollDuration",
        "shadeListCacheTTL"
      ]
    },
    {
      "type": "section",
      "title": "Performance Notes",
      "expandable": true,
      "expanded": false,
      "items": [
        {
          "type": "help",
          "helpvalue": "<ul><li><strong>HTTP Keep-Alive:</strong> Enabled automatically for 20-30% faster API calls</li><li><strong>Shade List Caching:</strong> Reduces unnecessary API calls during polling</li><li><strong>Adaptive Polling:</strong> Fast updates after activity, slower when idle</li></ul>"
        }
      ]
    }
  ]
}
